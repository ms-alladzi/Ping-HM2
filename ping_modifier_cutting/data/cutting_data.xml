<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_cutting" model="ir.sequence">
            <field name="name">Cutting Order</field>
            <field name="code">material.cutting</field>
            <field name="prefix">CO/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="sequence_internal_move" model="ir.sequence">
            <field name="name">Internal Move</field>
            <field name="code">internal.move</field>
            <field name="prefix">IM/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <record id="sequence_pickup_request" model="ir.sequence">
            <field name="name">Pickup Request</field>
            <field name="code">pickup.request</field>
            <field name="prefix">PickUp/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_delivery_request" model="ir.sequence">
            <field name="name">Delivery Request</field>
            <field name="code">delivery.request</field>
            <field name="prefix">Delivery/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_membership" model="ir.sequence">
            <field name="name">Membership</field>
            <field name="code">membership</field>
            <field name="prefix">%(range_y)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <record id="sequence_sample_request" model="ir.sequence">
            <field name="name">Sample Request</field>
            <field name="code">sample.request</field>
            <field name="prefix">Sample-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
       
        <record id="sequence_vendor_pricelist" model="ir.sequence">
            <field name="name">Vendor Pricelist</field>
            <field name="code">vendor.pricelist</field>
            <field name="prefix">VP-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>
